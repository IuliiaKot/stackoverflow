<ul class='vote-buttons'>
  <li>
    <form method="post" action='/answers/<%= answer.id %>/upvote' class="inline">
      <button type="submit" name="submit_param" value="submit_value" class="fa fa-sort-desc vote-button upvote-button"></button>
    </form>
  </li>
  <li class='vote-count'><%= answer.votes.count%></li>
  <li><a href="#">Dw</a></li>
</ul>
<section class='answer'>

  <p class='answer-body'><%= answer.body %></p>
  <span class='info'>answered <%= answer.created_at %> by <%= answer.user.username%></span>
  <a href="#" class='add-comment'>Add comment</a>

</section>
